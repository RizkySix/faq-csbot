// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model FAQ {
  id        String     @default(cuid()) @id
  question     String
  answer   String?
  downloaded Boolean @default(false)
}
